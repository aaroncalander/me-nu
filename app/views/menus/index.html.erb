<div class="container">
<h1>Here is the Weekly Menu</h1>

<div class="row">
  <div class="col s3">
    <h5>Appetizers</h5>
    <ul>
      <% @appetizers.each do |appetizer|%>
      <p><%= appetizer.name %></p>
      <% end %>
    </ul>
  </div>
  <div class="col s3">
    <h5>Salads</h5>
    <ul>
      <% @salads.each do |salad|%>
      <p><%= salad.name %></p>
      <% end %>
    </div>
  <div class="col s3">
    <h5>Entrees</h5>
      <ul>
    <% @entrees.each do |entree|%>
      <p><%= entree.name %></p>
    <% end %>
  </ul>
  </div>
  <div class="col s3">
    <h5>Desserts</h5>
    <ul>
    <% @desserts.each do |dessert|%>
      <p><%= dessert.name %></p>
    <% end %>
  </ul>
  </div>
</div>
